    public override int DoInit()
    {


      //=======//
	  //Protection
	  //=======//
      var ProtectionVAR = ElementByXPath("//div[1]/p[1 and contains (text(), 'Balance: N') ]  | //div[1]/p[1 and text()='Balance: 0 LITECOIN'] | //h5[contains(text(),'maintanace')] "); // 
				
