         {
           var TimeVAR = Time_Function();
           if (TimeVAR > 0) 
           {
               return TimeVAR;
           }
		 }// END MinuteVAR
		   
	     //=======//
		
			
        //Let Faucet Collector continue.
        return base.DoInit();
    }



